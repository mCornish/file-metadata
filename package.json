{
  "name": "mc-metadata",
  "version": "1.0.0",
  "description": "Receive metadata for an uploaded file",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run watch' 'node index.js'",
    "test": "babel-node **/*-test.js | faucet",
    "serve": "browser-sync start --proxy http://localhost:5000 --files *.{js,jsx,html,css} && node index.js",
    "watch": "npm-watch"
  },
  "watch": {
    "test": "src/*{.js,.jsx}"
  },
  "repository": {
    "url": "https://github.com/mCornish/file-metadata.git",
    "type": "git"
  },
  "author": "Mike Cornish",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "faucet": "^0.0.1",
    "mongodb": "^2.2.10",
    "multer": "^1.2.0",
    "tape": "^4.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "concurrently": "^3.1.0",
    "npm-watch": "^0.1.6",
    "watch": "^1.0.1"
  }
}
